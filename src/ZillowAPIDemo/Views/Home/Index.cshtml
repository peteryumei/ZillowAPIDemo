@{
    ViewData["Title"] = "Home Page";
}
<div class="container">
    <div class="row">
        <form action="/Form/FormOne" method="post" class="form-line">
            <h3>Please enter a home address to search</h3>
            <fieldset>

                <div class="form-group">
                    <!-- Street Address -->
                    <label for="street" class="control-label col-sm-2">Street Address: </label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="street" name="full_name" placeholder="Please enter street address">
                    </div>
                </div>

                <div class="form-group">
                    <!-- City -->
                    <label for="city" class="control-label col-sm-2">City: </label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="city" name="city" placeholder="Please enter city name">
                    </div>
                </div>

                <div class="form-group">
                    <!-- State -->
                    <label for="state" class="control-label col-sm-2">City: </label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="state" name="state" placeholder="Please enter state">
                    </div>
                </div>

                <div class="form-group">
                    <!-- State -->
                    <label for="zipcode" class="control-label col-sm-2">Zip Code: </label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" id="zipcode" name="zipcode" placeholder="Please enter zip code">
                    </div>
                </div>
           

                <div class="form-group">
                    <button type="submit" class="btn btn-primary">Submit</button>
                </div>

                </fieldset>
</form>

    </div>
  </div>





